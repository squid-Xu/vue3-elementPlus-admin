        "color": "^4.2.3",
        "echarts": "^5.5.1",
        "element-plus": "^2.8.0",
        "path-browserify": "^1.0.1",
        "path-to-regexp": "^7.1.0",
        "pinia": "^2.2.1",
        "vue": "^3.4.35",
        "vue-router": "^4.0.13"
    },
    "devDependencies": {
        "@eslint/js": "^9.9.0",
        "@iconify-json/ep": "^1.1.16",
        "@vitejs/plugin-vue": "^5.1.2",
